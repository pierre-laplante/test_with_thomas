{{
	function fact(n)
		/*
			@title factorial

			Compute a recursive factorial.

			@version 1.0 2018-04-18 laplante@sednove.com Initial version
			@version 1.1 2018-04-18 thomas@sednove.com Added digit check
		*/
		if isdigit(n) then 
			if (n<=0) then
				return 1;
			else
				return n*fact(n-1);
			endif
		endif
	endf

	fact(5);
}}
